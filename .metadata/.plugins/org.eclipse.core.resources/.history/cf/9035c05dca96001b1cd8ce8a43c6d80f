public interface TipoDestinoService {

	public void guardarTipoDestino(TipoDestino destino) throws Exception;

	public void eliminarTipoDestino(TipoDestino destino) throws Exception;

	public void actualizarTipoDestino(TipoDestino destino) throws Exception;

	public List<TipoDestino> consultarTiposDestino() throws Exception;
}
